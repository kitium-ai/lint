{
  "name": "@kitiumai/lint",
  "version": "1.3.5",
  "description": "Enterprise-ready, simple and secure linting configuration package for Kitium AI",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./eslint": "./eslint/index.js",
    "./eslint/base": "./eslint/base.js",
    "./eslint/react": "./eslint/react.js",
    "./eslint/node": "./eslint/node.js",
    "./eslint/typescript": "./eslint/typescript.js",
    "./eslint/jest": "./eslint/jest.js",
    "./eslint/testing-library": "./eslint/testing-library.js",
    "./eslint/graphql": "./eslint/graphql.js",
    "./eslint/vue": "./eslint/vue.js",
    "./eslint/nextjs": "./eslint/nextjs.js",
    "./eslint/kitium": "./eslint/kitium.js",
    "./prettier": "./prettier/index.js",
    "./tsconfig": "./tsconfig.json",
    "./tsconfig/base": "./tsconfig/base.json",
    "./tsconfig/react": "./tsconfig/react.json",
    "./tsconfig/node": "./tsconfig/node.json",
    "./jest": "./jest/index.js",
    "./husky": "./husky/index.js",
    "./configs": "./configs/index.js"
  },
  "files": [
    "index.js",
    "eslint/",
    "prettier/",
    "tsconfig/",
    "jest/",
    "husky/",
    "examples/",
    "configs/",
    "scripts/",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "echo 'No TypeScript files to build - this package uses JavaScript only'",
    "typecheck": "echo 'No TypeScript files to typecheck - this package uses JavaScript only'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "node --test",
    "test:watch": "node --test --watch",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "verify": "pnpm lint && pnpm format && pnpm test",
    "prepack": "npm run lint:fix && npm run format:fix",
    "prepublish": "npm run test",
    "setup": "node scripts/postinstall.js",
    "setup:lint": "node scripts/postinstall.js",
    "migrate": "node scripts/migrate.js",
    "changeset": "ensure-changeset && changeset",
    "version": "changeset version",
    "publish:set-token": "set-npm-token",
    "publish:setup": "set-npm-token",
    "publish:check": "npm whoami --registry https://registry.npmjs.org || npm login --registry https://registry.npmjs.org",
    "publish:login": "npm login --registry https://registry.npmjs.org",
    "publish:package": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org",
    "publish:package:otp": "pnpm run publish:check && pnpm publish --access public --no-git-checks --registry https://registry.npmjs.org --otp",
    "publish:dry-run": "pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "publish:dry-run:token": "pnpm run publish:setup && pnpm publish --access public --no-git-checks --dry-run --registry https://registry.npmjs.org",
    "release": "pnpm version && pnpm install --lockfile-only"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "linting",
    "eslint",
    "prettier",
    "typescript",
    "config",
    "enterprise",
    "kitium",
    "ai",
    "security",
    "react",
    "node",
    "jest",
    "testing-library",
    "graphql",
    "vue",
    "nextjs",
    "monorepo",
    "kitiumai",
    "javascript",
    "eslint",
    "lint",
    "prettier"
  ],
  "author": "Kitium AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitium-ai/lint.git"
  },
  "bugs": {
    "url": "https://github.com/kitium-ai/lint/issues"
  },
  "homepage": "https://github.com/kitium-ai/lint#readme",
  "peerDependencies": {
    "eslint": "^9.15.0",
    "typescript": "^5.6.0",
    "graphql": "^16.8.0",
    "eslint-plugin-graphql": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "graphql": {
      "optional": true
    },
    "eslint-plugin-graphql": {
      "optional": true
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "typescript-eslint": "^8.0.0",
    "@eslint/js": "^9.14.0",
    "eslint-plugin-boundaries": "^4.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-testing-library": "^7.0.0",
    "eslint-plugin-vue": "^9.30.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "@next/eslint-plugin-next": "^15.0.0",
    "eslint-plugin-sonarjs": "^1.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-no-unsanitized": "^4.1.0",
    "prettier": "^3.6.2",
    "husky": "^9.0.10"
  },
  "optionalDependencies": {
    "eslint-plugin-graphql": "^4.0.0"
  },
  "devDependencies": {
    "@kitiumai/config": "^2.0.0",
    "@kitiumai/scripts": "^1.0.0",
    "eslint": "^9.15.0",
    "typescript": "^5.6.0",
    "graphql": "^16.8.0",
    "eslint-plugin-graphql": "^4.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "lodash.get": "npm:lodash@^4.17.21"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "graphql"
      ],
      "allowedVersions": {
        "graphql": "16 || 15",
        "eslint": "9"
      }
    }
  }
}
